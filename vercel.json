{
  "buildCommand": "npm run build && cd app && npm install && npm run codegen && npx expo export -p web",
  "outputDirectory": "app/dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/graphql",
      "destination": "/api/graphql"
    },
    {
      "source": "/(.*)",
      "destination": "/app/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
